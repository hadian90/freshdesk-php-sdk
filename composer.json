{
    "name": "runcloudio/freshdesk-php-sdk",
    "description": "PHP SDK for the Freshdesk API (v2)",
    "type": "library",
    "homepage": "https://github.com/RunCloudIO/freshdesk-php-sdk",
    "keywords": [
        "freshdesk",
        "customer service",
        "support",
        "help desk",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Fikrizaman",
            "email": "fikri@runcloud.io",
            "homepage": "https://runcloud.io"
        },
        {
            "name": "Abdul Hadi",
            "email": "hadian90@gmail.com",
            "homepage": "http://hadiantech.com"
        },
        {
            "name": "Matthew Clarkson",
            "email": "mpclarkson@hilenium.com",
            "homepage": "http://mpclarkson.github.io/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "evert/phpdoc-md": "~0.2.0"
    },
    "replace": {
        "mpclarkson/freshdesk-php-sdk": "<=0.3.1"
    },
    "autoload": {
        "psr-4": {
            "Freshdesk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Freshdesk\\tests\\": "tests/"
        }
    }
}
